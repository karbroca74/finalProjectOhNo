<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.karrye.meetsession">

    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity android:name=".activities.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activities.TabActivity"/>
        <activity android:name=".activities.RegisterActivity"/>
        <activity android:name=".activities.ProfileActivity"/>
        <activity android:name=".activities.SearchActivity"/>
        <activity android:name=".activities.MatchesActivity"/>
        <activity android:name=".activities.BecomeAPremiumUserActivity"/>
        <activity android:name=".activities.HelpActivity"/>
        <activity android:name=".activities.MessageBoardActivity"/>

    </application>

</manifest>
